def get_overall_high_score(scores_dictionary):
    highscore = 0
    if len(scores_dictionary) > 0:
        for key in scores_dictionary.keys():
            for value in scores_dictionary[key]:
                if value > highscore:
                    highscore = value
    return highscore
scores = {
    "player1":[2020,980], "player2":[1040,2022,525,1024]
}
print(get_overall_high_score(scores))
