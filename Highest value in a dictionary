def get_overall_high_score(scores_dictionary):
    highscore = 0
    if len(scores_dictionary) > 0:
        for ke in scores_dictionary.keys():
            for val in scores_dictionary[ke]:
                if val > highscore:
                    highscore = val
    return highscore
scores = {
    "Steven":[2020,980], "Jane":[1040,2022,525,1024]
}
print(get_overall_high_score(scores))
